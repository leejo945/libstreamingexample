package net.majorkernelpanic.example1;

import android.util.Log;

public class SLog {
   public static final boolean IS_ENABLE =true;
   public static final boolean IS_ENABLE2 =true;
   private static final String ATTACH_STR = "cyrus";
   public static void e(String tag, String msg){
	   if(IS_ENABLE){
		   Log.e(ATTACH_STR+tag, msg);
	   }
   }
   public static void d(String tag, String msg){
	   if(IS_ENABLE){
		   Log.d(ATTACH_STR+tag, msg);
	   }
   }
}
